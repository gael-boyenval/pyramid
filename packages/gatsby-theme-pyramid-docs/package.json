{
  "name": "@pyramid/gatsby-theme-pyramid-docs",
  "version": "0.0.0",
  "description": "a gatsby theme to create styelguide, design system or code documentations",
  "author": "gael-boyenval (gaelboyenval@icloud.com)",
  "homepage": "",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build:declaration": "tsc --project tsconfig.build.json",
    "build:es": "BABEL_ENV=build babel src --root-mode upward --out-dir lib --source-maps --extensions .ts,.tsx --delete-dir-on-start --no-comments",
    "build:es:watch": "BABEL_ENV=build babel -w src --root-mode upward --out-dir lib --source-maps --extensions .ts,.tsx --delete-dir-on-start --no-comments --verbose",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.5",
    "@mdx-js/react": "^1.6.5",
    "gatsby-plugin-flexsearch": "^1.0.3",
    "gatsby-plugin-mdx": "^1.2.15",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sass": "^2.0.1",
    "gatsby-plugin-styled-components": "^3.0.0",
    "gatsby-plugin-typescript": "^2.4.3",
    "gatsby-remark-autolink-headers": "^2.1.13",
    "gatsby-remark-static-images": "^1.1.0",
    "gatsby-source-filesystem": "^2.0.3",
    "gatsby-transformer-json": "^2.4.1",
    "gatsby-transformer-remark": "^2.1.7",
    "identity-obj-proxy": "^3.0.0",
    "prism-react-renderer": "^0.1.6",
    "prism-themes": "^1.0.1",
    "prismjs": "^1.15.0",
    "react-helmet": "^6.0.0",
    "react-transition-group": "^4.4.1",
    "remark": "^11.0.2"
  },
  "resolutions": {
    "remark": "11.0.2"
  },
  "peerDependencies": {
    "gatsby": "2.21.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.4",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^4.2.4",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^9.1.2",
    "core-js": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.5.0",
    "gatsby": "2.21.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "react-test-renderer": "^16.12.0",
    "styled-components": "^5.1.0"
  }
}
